<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.view_binding_tutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppName">
        <activity
            android:name=".groupsfragment.group_users.Group_Users"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar"
            android:exported="false" />
        <activity
            android:name=".userprofile.User_Profile_Activity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar"
            android:exported="false" />

        <activity
            android:name=".book_homepage.Book_home_page_Activity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>